<table class="generaltable" id="enrolcode-fullsize-{{uniqid}}">
    <tr>
        <th>
            {{#str}} url {{/str}}
        </th>
        <td colspan="2">
            <a href="{{url}}" target="_blank">{{url}}</a>
        </td>
    </tr>
    <tr>
        <th>{{#str}} role {{/str}}</th>
        <td class="role">{{role}}</td>
        <td rowspan="5">
            <a href="#" class="url">
                <img src="{{qr}}" class="qr" height="300"/>
            </a>
        </td>
    </tr>
    <tr>
        <th>{{#str}} group {{/str}}</th>
        <td class="group">{{group}}</td>
    </tr>
    <tr>
        <th>{{#str}} custommaturity, block_enrolcode {{/str}}</th>
        <td class="maturity">{{maturity}}</td>
    </tr>
    <tr>
        <th>{{#str}} enrolmentend, block_enrolcode {{/str}}</th>
        <td class="enrolmentend">{{enrolmentend}}</td>
    </tr>
    <tr>
        <th>{{#str}} code:accesscode, block_enrolcode {{/str}}</th>
        <td class="accesscode">{{accesscode}}</td>
    </tr>
</table>
